[prerules]
CC ?= gcc
COBJEXT = .o

UNIT_DIRS=../../utils/*

[rules]
include ../../../scripts/common.make
all: wrapper unit
	
wrapper: sockwrapc$(COBJEXT)
	
unit: sockwrap$(PPUEXT)

clean:
	$(DEL) $(wildcard *$(COBJEXT))

sockwrap.pas: sockwrapc.c
	$(TOUCH) $@

%$(COBJEXT): %.c
	$(CC) $(CFLAGS) -c -o $@ $<

%.c: %.h
	$(TOUCH) $@
